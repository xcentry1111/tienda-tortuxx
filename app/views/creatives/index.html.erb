<h1> Bienvenido</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
      <th>Descripcion</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @productos.each do |producto| %>
      <tr>
        <td><%= producto.nombre %></td>
        <td><%= producto.precio %></td>
        <td><%= producto.descripcion %></td>
        <td><%= producto.user %></td>
        <td><%= link_to 'Show', producto %></td>
      </tr>
    <% end %>
  </tbody>
</table>
	
<p><%=link_to "Cerrar Sesion", destroy_user_session_path, method: :delete %></p>	
	

